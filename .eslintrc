{
  "extends": [
    "airbnb-base"
  ],

  "plugins": [
    "import"
  ],

  "env": {
    "es6": true,
    "jquery": true,
    "jest": true
  },

  "settings": {
    "import/resolver": "webpack"
  },

  "globals": {
    "window": true
  },

  "rules": {
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "no-underscore-dangle": 0,
    "object-shorthand": 0,
    "no-multi-spaces": 0
  },
}
