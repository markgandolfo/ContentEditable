{
  "extends": [
    "airbnb-base"
  ],

  "plugins": [
    "import",
    "vue"
  ],

  "env": {
    "es6": true,
    "jest": true
  },

  "settings": {
    "import/resolver": "webpack"
  },

  "globals": {
    "window": true
  },

  "rules": {
    "import/no-duplicates": 0,
    "import/no-absolute-path": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "no-multi-assign": 0,
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "no-underscore-dangle": 0,
    "object-shorthand": 0,
    "no-multi-spaces": 0
  },
}
